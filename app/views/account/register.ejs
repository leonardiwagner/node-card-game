<% include ../header %>

<div class="row">
  <div class="col-md-4"></div>
  <div class="col-md-4">
    <div class="panel panel-primary">
      <div class="panel-heading">Register</div>
      <div class="panel-body">
        <form role="form">
        <div class="form-group">
          <label for="email">Email address</label>
          <input type="email" class="form-control input-lg" id="fldEmail">
        </div>
        <div class="form-group">
          <label for="password">Password</label>
          <input type="password" class="form-control input-lg" id="fldPassword">
        </div>
        <div class="form-group">
          <label for="fldPasswordConfirmation">Retype password</label>
          <input type="password" class="form-control input-lg" id="fldPasswordConfirmation">
        </div>
        <button type="submit" class="btn btn-primary btn-block btn-lg">Register</button>
      </form>
      </div>
    </div>
  </div>
</div>




<% include ../footer %>